{{includeTemplate=filter}}
<table class="classes list">
	<thead>
		<tr>
			<th>{{writeString=class.heading.name}}</th>
			<th>{{writeString=class.heading.class_type}}</th>
			<th>{{writeString=class.heading.teachers}}</th>
		</tr>
	</thead>
	<tbody data-wok-htmlSource="/classesData" data-wok-htmlFiltered="classesData.filtered">
		{{items}}
	</tbody>
</table>



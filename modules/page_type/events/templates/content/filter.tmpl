<div data-wok-filter='eventData/eventData.filtered/{"year": {{writeDate=Y}}, "month": 0, "view": "list", "granularity": "year", "kind": "event"},"session"'>
	<h1 data-wok-filter-title='filter//"year"'></h1>
	<div class="filters">
		<div data-wok-filter-options='filter//"kind"'>
			<ul class="available">
				<li data-value="">{{writeString=appointments.kind.all}}</li>
				<li data-value="event">{{writeString=appointments.kind.events}}</li>
				<li data-value="date">{{writeString=appointments.kind.dates}}</li>
			</ul>
		</div>
		<div data-wok-filter-date-pager='filter//"date_start","date_end", null, "granularity", ["{{writeDate=B;date=1-1;format=n-j}}", "{{writeDate=B;date=2-1;format=n-j}}", "{{writeDate=B;date=3-1;format=n-j}}", "{{writeDate=B;date=4-1;format=n-j}}", "{{writeDate=B;date=5-1;format=n-j}}", "{{writeDate=B;date=6-1;format=n-j}}", "{{writeDate=B;date=7-1;format=n-j}}", "{{writeDate=B;date=8-1;format=n-j}}", "{{writeDate=B;date=9-1;format=n-j}}", "{{writeDate=B;date=10-1;format=n-j}}", "{{writeDate=B;date=11-1;format=n-j}}", "{{writeDate=B;date=12-1;format=n-j}}"]'>
			<span class="prev wett-icon">&lt;</span>
			<span class="current"></span>
			<span class="next wett-icon">&gt;</span>
		</div>
		<div data-wok-filter-toggle='filter//"granularity"'>
			<div data-value="month">{{writeString=month}}</div>
			<div data-value="year">{{writeString=year}}</div>
		</div>
		<div data-wok-filter-toggle='filter//"view"'>
			<div data-value="calendar" class="wett-icon">grid</div>
			<div data-value="list" class="wett-icon">list</div>
		</div>
	</div>
</div>
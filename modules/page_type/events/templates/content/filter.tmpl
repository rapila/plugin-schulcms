<div data-wok-filter='eventData/eventData.filtered/{"year": {{writeDate=Y}}, "view": "calendar", "granularity": "year"}'>
	<h1 data-wok-filter-title='filter//"year"'></h1>
	<div class="filters">
		<div data-wok-filter-date-pager='filter//"date_start","date_end", null, "granularity", ["{{writeDate=B;date=1;format=n}}", "{{writeDate=B;date=2;format=n}}", "{{writeDate=B;date=3;format=n}}", "{{writeDate=B;date=4;format=n}}", "{{writeDate=B;date=5;format=n}}", "{{writeDate=B;date=6;format=n}}", "{{writeDate=B;date=7;format=n}}", "{{writeDate=B;date=8;format=n}}", "{{writeDate=B;date=9;format=n}}", "{{writeDate=B;date=10;format=n}}", "{{writeDate=B;date=11;format=n}}", "{{writeDate=B;date=12;format=n}}"]'>
			<span class="prev wett-icon">&lt;</span>
			<span class="current">
				<span rv-if="value.granularity_year">{ value.year }</span>
				<span rv-if="value.granularity_month">{ value.month | month }</span>
			</span>
			<span class="next wett-icon">&gt;</span>
		</div>
		<div data-wok-filter-toggle='filter//"granularity"'>
			<div data-value="month">{{writeString=month}}</div>
			<div data-value="year">{{writeString=year}}</div>
		</div>
		<div data-wok-filter-toggle='filter//"view"'>
			<div data-value="calendar" class="wett-icon">grid</div>
			<div data-value="list" class="wett-icon">list</div>
		</div>
	</div>
</div>
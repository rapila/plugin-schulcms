<p>{{writeParameterizedString=main_search_result;search_string=\{\{search_string\}\};count=\{\{count\}\};position=last}}</p>
{{if=>;1=\{\{count\}\};2=0}}
<table>
	<thead>
	<tr><th>{{writeString=wns.page}}</th><th>{{writeString=wns.search_hits}}</th></tr>
	</thead>
	<tbody>
		{{items}}
	</tbody>
	<tfoot>
	</tfoot>
</table>
{{endIf}}
{{identifierContext=start;name=no_results}}
	<em class="info">
		{{no_results}}
	</em>
{{identifierContext=end;name=no_results}}